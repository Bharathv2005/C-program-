#include<stdio.h>
int main()
{
    int age;
    printf("Enter your age: ");
    scanf("%d",&age);
    if(age>=18 && age<60)
    {
        printf("The person is adult");
    }
    else if(age<18)
    {
        printf("The person is child");
    }
    else if (age>=60)
    {
        printf("The person is senior citizen");
    }
}
