#include <stdio.h>
int main() {
   int age, usn;
   char name, clg;
   float weight, height;
   printf("STUDENT DETAILS\n");
   printf("Enter name: ");
   scanf("%s",&name);
   printf("Enter your age: ");
   scanf("%d",&age);
   printf("Enter your weight: ");
   scanf("%f",&weight);
   printf("Enter your height: ");
   scanf("%f",&height);
   printf("Enter your clg: ");
   scanf("%s",&clg);
   printf("Enter your usn: ");
   scanf("%d",&usn);
}
