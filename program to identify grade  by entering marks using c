#include<stdio.h>
int main()
{
    float marks;
    printf("Enter the marks: ");
    scanf("%f",&marks);
    if (marks>=85)
    {
        printf("grade A");
    }
    else if(marks>=70)
    {
        printf("grade B");
    }
    else if(marks>=60)
    {
        printf("grade c");
    }
    else
    {
        printf("grade D");
    }
}
